<a class="bench-update" 
    wbid="[% c.stash.star.wbid %]" 
    href="/rest/workbench" 
    name="[% c.stash.star.name %]" 
    objclass="[% c.stash.star.class %]" 
    is_obj="[% c.stash.star.is_obj %]" 
    url="[% c.stash.star.url | uri %]">
<div id="save" class="ui-icon ui-icon-stars
[% IF c.stash.star.value == 1 %]
  ui-icon-star-yellow" title="remove this report" 
[% ELSE %]
  ui-icon-star-gray" title="save this report" 
[% END %]
></div>
</a>   