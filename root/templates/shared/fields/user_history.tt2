
[% FOREACH h IN c.stash.history %]
  <li><div class="ellipsis">
    [% linkPage(h.page) %]
    
   <span id="fade"> - [% h.time_lapse %] 
  [% IF h.visits > 1 %]([% h.visits %] visits) [% END %]
  </span>
</div>
  </li>
[% END %]
  <li><a id="fade" onclick="history_clear()">clear history</span></li>